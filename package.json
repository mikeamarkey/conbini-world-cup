{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "check:format": "prettier --check --ignore-path .gitignore .",
    "check:lint": "eslint --cache .",
    "check:types": "tsc -b",
    "check": "run-p \"check:*\"",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages --compatibility-date=2022-12-07 dev ./public --binding $(cat .env)",
    "dev": "remix build && run-p \"dev:*\"",
    "fix:format": "npm run check:format -- --write",
    "fix:lint": "npm run check:lint -- --fix",
    "fix": "run-p \"fix:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.8.1",
    "@remix-run/cloudflare-pages": "^1.8.1",
    "@remix-run/react": "^1.8.1",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.8.1",
    "@remix-run/eslint-config": "^1.8.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "eslint": "^8.29.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3",
    "wrangler": "^2.6.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
